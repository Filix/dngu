{% block all %}
<!DOCTYPE html>
<html lang="en">
    {% block head_tag %}
    <head>
        <meta charset="utf-8" />
        <title>{% block title %}Better.com{% endblock title %}</title> 
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <meta name="description" content="{% block description %}Better.com{% endblock description %}" />
        <meta name="keywords" content="{% block keywords %}Better.com{% endblock keywords %}" />
        <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
        {% block head_css %}
        {% stylesheets
                '@DnguWebBundle/Resources/public/css/bootstrap/bootstrap.css'
                '@DnguWebBundle/Resources/public/css/bootstrap/bootstrap-responsive.css'
                '@DnguWebBundle/Resources/public/css/style.css'
                '@DnguWebBundle/Resources/public/css/layerslider.css'
         %}
        <link rel="stylesheet"  href="{{ asset_url }}" >
        {% endstylesheets %}
        <!--[if lt IE 9 ]>
        <link href="css/styleIE.css" rel="stylesheet">
        <![endif]-->
        <!--[if IE 9 ]>
        <link href="css/styleIE9.css" rel="stylesheet">
        <![endif]-->
        {% endblock head_css %}
        {% block head_script %}
        {% javascripts
                
        %}
        <script charset="utf-8" src="{{ asset_url }}"></script> 
        {% endjavascripts %}
        <!--[if lt IE 9]>
          <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
        <![endif]-->
        {% endblock head_script %}
      </head>
      {% endblock head_tag %}
    <body>
        {% block header %}
        <!--start: Header -->
        <header>
            <div class="container">
                <div class="navbar navbar-inverse">
                    <div class="navbar-inner">
                        <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </a>
                        <a class="brand" href="index.html">Better.com</a>
                        <div class="nav-collapse collapse">
                            <ul class="nav">
                                <li class="active"><a href="index.html">首页</a></li>
                                <li><a href="album.html">相册</a></li>
                                <li><a href="diaries.html">日志</a></li>	
                                <li><a href="wiki.html">Wiki</a></li>								
                            </ul>
                        </div>
                        <div class="nav pull-right">
                            <ul class="nav">
                                <li><a href="{{ path('login') }}">登陆</a></li>
                                <li><a href="{{ path('register') }}">注册</a></li>
                            </ul>
                        </div>
                    </div>
                </div>	
            </div>
        </header>
        <!--end: Header-->
        {% endblock header %}
        
        {% block container %}
        <!--start: Container -->
        <div class="container main-content">
            {% block content %}
            {% block page_title %}
            <div id="page-title">
                <h2>Title</h2>
            </div>
            {% endblock page_title %}
            {% block wrapper %}
            <div id="wrapper">

            </div>
            {% endblock wrapper %}
            {% endblock content %}
        </div>
        <!-- end: Container  -->	
        {% endblock container %}

        {% block footer %}
        <!-- start: Under Footer -->
        <div id="under-footer" class="navbar-fixed-bottom">
            <div class="container">
                <div class="row-fluid">
                    <div class="span2">
                        <div id="under-footer-logo">
                            <a class="brand" href="/index.html">Better.com</a>
                        </div>
                    </div>
                    <div class="span9">
                        <div id="under-footer-copyright">
                            &copy; 2013, <a href="http://clabs.co">creativeLabs</a>. Designed by <a href="http://clabs.co">creativeLabs</a> 
                        </div>
                    </div>
                    <div class="span1">
                        <div id="under-footer-back-to-top">
                            <a href="#"></a>
                        </div>
                    </div>
                </div>
            </div>	
        </div>
        <!-- end: Under Footer -->	
        {% endblock footer %}
        {% block tail_script %}
        {% javascripts
                '@DnguWebBundle/Resources/public/js/jquery/jquery-1.8.2.js'
                '@DnguWebBundle/Resources/public/js/bootstrap/bootstrap.js'
        %}
        <script charset="utf-8" src="{{ asset_url }}"></script> 
        {% endjavascripts %}
        {% endblock tail_script%}

    </body>
</html>
{% endblock all %}